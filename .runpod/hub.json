{
  "title": "Qwen Background Remover",
  "description": "AI-powered background removal using Qwen-Image-Lightning",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://huggingface.co/lightx2v/Qwen-Image-Lightning/resolve/main/thumbnail.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 20,
    "gpuCount": 1,
    "presets": [
      {
        "name": "T4 Default",
        "defaults": {
          "MODEL_NAME": "lightx2v/Qwen-Image-Lightning",
          "DEVICE": "cuda",
          "PORT": "8080"
        }
      }
    ],
    "env": [
      {
        "key": "MODEL_NAME",
        "input": {
          "name": "HF Model ID",
          "type": "string",
          "description": "Hugging Face model repo",
          "default": "lightx2v/Qwen-Image-Lightning"
        }
      }
    ]
  }
}
